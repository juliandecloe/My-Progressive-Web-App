<%- include('./partials/head') %>

<%- include('./partials/header') %>

<main>
    <section class="search hide">
        <h2>Search results</h2>
        <section class="no-results hide">
            <p>There are no results!</p>
            <button>Try again</button>
        </section>
        <% data.forEach((item) => { %>
        <article id="<%= item.objectNumber %>">
                <h3><%= item.title %></h3>
                <p><%= item.principalOrFirstMaker %></p>
                <img src="<%= item.webImage.url.slice(0, -3)+"=s1000" %>" alt="<%= item.title %>">
        </article>
        <% }) %>
        <form action="/page=<%= page %>"><button>Load more</button></form> 
    </section>
</main>

<%- include('./partials/foot') %>

